{
  "name": "next-view-example",
  "version": "2.0.0",
  "description": "The Augmented.js Next - View Example",
  "main": "public/bundle.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader'",
    "build": "webpack --mode production --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader'",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "jsdoc ./src -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/next-view-exmaple.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation",
    "example"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "compression": "^1.7.2",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.9.0",
    "mocha": "^5.1.1",
    "node-fetch": "^2.1.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "augmentedjs-next-presentation": "^2.0.14"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/next-view-exmaple/issues"
  },
  "homepage": "http://www.augmentedjs.com"
